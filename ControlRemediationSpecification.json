{
    "Version": "<version>",
    "Description": "Specifies metadata about Remediation scripts",
    "RemediationScriptsBaseUrl": "https://aka.ms",
    "RemediationSpecification": [
        {
            "ControlId": "Azure_APIManagement_AuthN_Disable_Management_API",
            "DisableRemediation": "false",
            "Filename": "<remediation-script-name>",
            "RemediationScriptUrl": "<url-remediation-script-ps1>",
            "StartCommand": "<start-command-with-args>"

        },
        {
            "ControlId": "Azure_APIManagement_DP_Use_HTTPS_URL_Scheme",
            "DisableRemediation": "true",
            "Filename": "<remediation-script-name>",
            "RemediationScriptUrl": "<url-remediation-script-ps1>",
            "StartCommand": "<start-command-with-args>"
        }, 

        {
            "ControlId": "Azure_Storage_AuthN_Dont_Allow_Anonymous",
            "DisableRemediation": "true",
            "Filename": "<remediation-script-name>",
            "RemediationScriptUrl": "<url-remediation-script-ps1>",
            "StartCommand": "<start-command-with-args>"
        }
    ]
}